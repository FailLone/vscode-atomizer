{
    "name": "atomizer",
    "displayName": "Atomizer",
    "description": "A tool to automatically generate Atomic CSS.",
    "version": "0.3.2",
    "publisher": "pankaj-parashar",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/acss-io/vscode-atomizer"
    },
    "galleryBanner": {
        "color": "#cfe7ef",
        "theme": "light"
    },
    "engines": {
        "vscode": "^0.10.10"
    },
    "categories": [
        "Other",
        "Snippets"
    ],
    "keywords": [
        "Atomic CSS",
        "ACSS",
        "Atomizer",
        "CSS",
        "Snippets",
        "Yahoo",
        "VSCode"
    ],
    "icon": "logo.png",
    "activationEvents": [
        "workspaceContains:atomizer.json"
    ],
    "main": "./extension",
    "contributes": {
        "snippets": [
            {
                "language": "html",
                "path": "snippets.json"
            },
            {
                "language": "javascript",
                "path": "snippets.json"
            },
            {
                "language": "javascriptreact",
                "path": "snippets.json"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "dependencies": {
        "atomizer": "3.3.4"
    },
    "devDependencies": {
        "vscode": "^0.11.0"
    }
}